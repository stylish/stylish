{
  "name": "@architects/pdf-demo",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/architects/skypager-central"
  },

  "skypager":{
    "plugins":[ 
      "blueprints",
      "integrations"
    ],
    "main": "skypager.js",
    "theme": "dashboard",
    "domain": "pdf-demo.skypager.io",
    "platform": "web",
    "featureFlags":{
      "API_BASE_URL": "http://localhost:3000"
    },
    "staticAssets": {
      "headerScripts":[
        "https://cdn.auth0.com/js/lock-8.2.js"
      ]
    }
  },

  "scripts": {
    "bundle": "skypager export bundle",
    "predev": "npm run bundle",
    "dev": "DEBUG_SERVER=true skypager dev",
    "prebuild": "npm run bundle",
    "build": "skypager build --push-state",
    "deploy": "skypager publish"
  },

  "dependencies": {
    "skypager-electron": "latest",
    "skypager": "latest",
    "skypager-application": "latest",
    "skypager-ui": "latest",
    "skypager-themes": "latest"
  },

  "devDependencies": {
    "skypager-devpack": "latest",
    "skypager-plugin-blueprints": "latest"
  }

}
