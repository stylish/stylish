{
  "name": "@architects/webapp-boilerplate",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/architects/skypager-central"
  },
  "skypager":{
    "plugins":[ "blueprints" ],
    "theme": "dashboard-dark",
    "domain": "webapp-boilerplate.skypager.io"
  },
  "scripts": {
    "dev": "DEBUG_SERVER=true skypager serve --use-precompiled-template dashboard-dark",
    "build": "skypager compile --push-state",
    "deploy": "skypager publish"
  }
}
