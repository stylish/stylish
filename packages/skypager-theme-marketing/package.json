{
  "name": "@architects/skypager-theme-marketing",
  "private":"true",
  "version": "2.12.0",
  "description": "a theme for marketing websites",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r 'babel-register' -r 'should' test/**/*-spec.js",
    "start": "skypager serve web",
    "prepublish": ""
  },
  "author": "Jon Soeder <jon@chicago.com>",
  "license": "SEE LICENSE IN License.md",
  "skypager": {
    "main": "skypager.js",
    "provides":[
       "theme",
       "theme:marketing",
       "theme:marketing-minimal",
       "theme:marketing-bold"
    ]
  },
  "devDependencies": {
    "skypager-project": "latest",
    "babel-preset-skypager": "latest",
    "mocha": "latest",
    "should": "latest"
  }
}
