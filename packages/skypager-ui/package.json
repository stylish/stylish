{
  "name": "skypager-ui",
  "description": "collection of ui layouts and components styled by a data-driven bootstrap theme generator",
  "version": "1.30.0",
  "main": "index.js",
  "skypager": {
    "main": "./skypager.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/architects/skypager-central"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha --require 'babel-register' --require should --require ./test/index.js test/**/*-spec.js",
    "build:clean": "rm -rf lib dist",
    "build": "npm run build:clean && npm run test && npm run compile",
    "compile": "NODE_ENV=production skypager build --output-folder lib --skip-theme --no-content-hash --webpack-config=./webpack.bundle.config.js --entry-only" 
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.3",
    "history": "^1.17.0",
    "jquery": "^2.2.0",
    "react": "^0.14.7",
    "react-body-classname": "latest",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^1.0.3",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^1.0.3",
    "skypager-themes": "^1.29.0",
    "lodash": "^3.4.0"
  },
  "devDependencies": {
    "babel-preset-skypager": "^1.29.0"
  }
}
