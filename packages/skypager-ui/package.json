{
  "name": "skypager-ui",
  "description": "collection of ui layouts and components styled by a data-driven bootstrap theme generator",
  "version": "1.20.0",
  "main": "index.js",
  "skypager": {
    "main": "./skypager.js"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha --require 'babel-register' --require should --require ./test/index.js test/**/*-spec.js",
    "build:clean": "rm -rf lib dist",
    "build": "npm run build:clean && npm run test && npm run compile",
    "compile": "NODE_ENV=production babel -q -d lib src"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.3",
    "history": "^1.17.0",
    "jquery": "^2.2.0",
    "react": "^0.14.6",
    "react-body-classname": "latest",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.1",
    "redux-simple-router": "^2.0.2",
    "redux-thunk": "^1.0.3",
    "skypager-application": "^1.0.3",
    "skypager-themes": "^1.0.3"
  },
  "devDependencies": {
    "skypager": "^1.17.1",
    "skypager-devpack": "^1.17.1",
    "babel-runtime": "^6.3.13",
    "babel-preset-skypager": "^2.0.3"
  }
}
