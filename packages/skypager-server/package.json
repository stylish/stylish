{
  "name": "skypager-server",
  "description": "skypager project server",
  "version": "1.34.0",
  "main": "index.js",
  "scripts": {
    "compile": "SKYPAGER_ENV=release babel -q -d lib src",
    "compile:watch": "SKYPAGER_ENV=release babel -w -d lib src",
    "test": "SKYPAGER_ENV=test NODE_ENV=test mocha --require 'babel-register' --require should --require ./test/index.js test/**/*-spec.js",
    "pretest:watch": "SKYPAGER_ENV=development npm run test:bundle",
    "test:watch": "SKYPAGER_ENV=development NODE_ENV=test mocha --growl --watch --require 'babel-register' --require ./test/index.js test/**/*-spec.js",
    "preversion": "npm run test",
    "version": "npm run compile && git add -A lib"
  },
  "skypager": {
    "main": "skypager.js"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "colorz": "^0.1.9",
    "deepstream.io": "^0.2.16",
    "deepstream.io-cache-redis": "^0.2.5",
    "deepstream.io-client-js": "^0.3.8",
    "deepstream.io-msg-redis": "^0.2.7",
    "fs-extra-promise": "^0.3.1",
    "json-colorz": "^0.2.3",
    "lodash": "^4.3.0",
    "nodemon": "^1.8.1",
    "pty.js": "^0.3.0",
    "react": "^0.14.7",
    "react-blessed": "^0.1.7",
    "shelljs": "^0.6.0",
    "skypager": "^1.34.0",
    "term.js": "0.0.7",
    "webpack": "^1.12.13"
  },
  "devDependencies": {
    "babel-preset-skypager": "^1.34.0",
    "mocha": "^2.3.4",
    "should": "latest",
    "skypager-devpack": "^1.34.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
