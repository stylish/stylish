{
  "name": "skypager-themes",
  "description": "manage a library of themes to use with skypager projects. variables for the themes can be configured via an external yaml file.",
  "version": "2.9.1",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/skypager/skypager"
  },
  "loader": "index.loader.js",
  "skypager": {
    "main": "skypager.js",
    "provides":[
      "devtools",
      "webpack-loader"
    ]
  },
  "scripts": {
    "test": "exit 0",
    "build:clean": "rm -rf lib dist",
    "build": "npm run build:clean && npm run test && npm run compile",
    "compile": "babel -q -d lib src",
    "prepublish": "npm run compile && git add . -A"
  },
  "author": "Jonathan Soeder <jon@chicago.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "expose-loader": "^0.7.1",
    "lodash": "^4.1.0",
    "js-yaml": "^3.4.5",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "css-loader": "^0.23.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "file-loader": "^0.8.5"
  },
  "devDependencies": {
    "babel-preset-skypager": "^2.2.1"
  }
}
