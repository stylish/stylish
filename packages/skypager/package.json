{
  "name": "skypager",
  "version": "1.17.0",
  "description": "skypager.io core framework project and helpers",
  "main": "index.js",
  "bin": {
    "skypager": "./bin/cli.js"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha --require 'babel-register' --require should --require ./test/index.js test/**/*-spec.js",
    "test:watch": "NODE_ENV=test mocha --growl --watch --require 'babel-register' --require ./test/index.js test/**/*-spec.js",
    "lint": "eslint src/**/*",
    "lint:fix": "npm run lint -- --fix",
    "build:clean": "rm -rf lib test/fixture/dist/*",
    "build": "npm run build:clean && npm run test && npm run compile",
    "compile": "NODE_ENV=production babel -q -d lib src",
    "compile:watch": "babel -w -d lib src",
    "console": "./bin/skypager.js repl --project-path ./test/fixture"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/architects/skypager"
  },
  "keywords": [
    "skypager"
  ],
  "author": "Jonathan Soeder",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/architects/skypager/issues"
  },
  "homepage": "https://github.com/architects/skypager#readme",
  "skypager": {
    "main": "test/fixture/index.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "cheerio": "^0.19.0",
    "cheerio-select": "0.0.3",
    "chokidar-cli": "^1.2.0",
    "colors": "^1.1.2",
    "css": "^2.2.1",
    "csv": "^0.4.6",
    "debug": "^2.2.0",
    "dotenv": "^1.2.0",
    "fs-promise": "^0.3.1",
    "glob": "^6.0.1",
    "i": "^0.3.3",
    "is-domain": "0.0.1",
    "js-yaml": "^3.4.5",
    "lodash": "^3.10.1",
    "md5": "^2.0.0",
    "mdast": "^2.2.2",
    "mdast-html": "^1.2.1",
    "mdast-normalize-headings": "^0.2.0",
    "mdast-squeeze-paragraphs": "^1.1.0",
    "mdast-yaml": "^1.0.0",
    "minimatch": "^3.0.0",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.0.1",
    "object-path": "^0.9.2",
