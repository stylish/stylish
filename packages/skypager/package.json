{
  "name": "skypager",
  "version": "1.34.0",
  "description": "skypager.io core framework project and helpers",
  "main": "index.js",
  "bin": {
    "skypager": "./bin/cli.js"
  },
  "scripts": {
    "test:bundle:export": "skypager export bundle --clean",
    "test:bundle:compile": "webpack --config test/webpack.bundle.config.js > /dev/null",
    "test:bundle": "npm run test:bundle:export && npm run test:bundle:compile",
    "pretest": "SKYPAGER_ENV=development npm run test:bundle",
    "test": "SKYPAGER_ENV=test NODE_ENV=test mocha --require 'babel-register' --require should --require ./test/index.js test/**/*-spec.js",
    "pretest:watch": "SKYPAGER_ENV=development npm run test:bundle",
    "test:watch": "SKYPAGER_ENV=development NODE_ENV=test mocha --growl --watch --require 'babel-register' --require ./test/index.js test/**/*-spec.js",
    "lint": "eslint src/**/*",
    "lint:fix": "npm run lint -- --fix",
    "dev:console": "./bin/skypager.js repl --dev-mode --project ./test/fixture",
    "compile": "SKYPAGER_ENV=release babel -q -d lib src",
    "compile:watch": "SKYPAGER_ENV=release babel -w -d lib src",
    "preversion": "npm run test",
    "version": "npm run compile && git add -A lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/architects/skypager"
  },
  "keywords": [
    "skypager"
  ],
  "author": "Jonathan Soeder",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/architects/skypager/issues"
  },
  "homepage": "https://github.com/architects/skypager#readme",
  "skypager": {
    "main": "test/fixture/index.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "cheerio": "^0.19.0",
    "chokidar-cli": "^1.2.0",
    "colors": "^1.1.2",
    "css": "^2.2.1",
    "fs-promise": "^0.3.1",
    "git-rev": "^0.2.1",
    "glob": "^6.0.1",
    "i": "^0.3.3",
    "js-yaml": "^3.4.5",
    "lodash": "^3.10.1",
    "md5": "^2.0.0",
    "mdast": "^2.2.2",
    "mdast-html": "^1.2.1",
    "mdast-normalize-headings": "^0.2.0",
    "mdast-squeeze-paragraphs": "^1.1.0",
    "mdast-yaml": "^1.0.0",
    "minimatch": "^3.0.0",
    "mkdirp": "^0.5.1",
    "path-match": "^1.2.3",
    "path-to-regexp": "^1.2.1",
    "pkginfo": "^0.3.1",
    "read": "^1.0.7",
    "request": "^2.67.0",
    "rimraf": "^2.5.1",
    "should": "^7.1.1",
    "unist-util-visit": "^1.0.0",
    "url": "^0.11.0",
    "utile": "^0.3.0",
    "yargs": "^3.31.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta4",
    "babel-preset-skypager": "^1.24.0",
    "colors": "^1.1.2",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-standard": "^1.3.1",
    "mocha": "^2.3.4",
    "react": "^0.14.7",
    "webpack": "^1.12.13"
  },
  "peerDependencies": {
    "skypager-devpack": "^1.33.0"
  }
}
