{
  "name": "skypager-application",
  "description": "unified react and redux appication runtime for skypager projects supporting the browser, electron, and react native",
  "version": "1.31.0",
  "main": "index.js",
  "skypager": {
    "main": "skypager.js"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha --require 'babel-register' --require should --require ./test/index.js test/**/*-spec.js",
    "build:clean": "rm -rf lib dist",
    "build": "npm run build:clean && npm run test && npm run compile",
    "compile": "NODE_ENV=production babel -q -d lib src"
  },
  "keywords": [
    "react",
    "react-native",
    "redux",
    "electron",
    "skypager"
  ],
  "dependencies": {
    "invariant": "^2.2.0",
    "history": "^1.17.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.1",
    "redux-simple-router": "^2.0.2",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "mocha": "^2.4.5",
    "should": "^8.2.1",
    "sinon": "^1.17.3"
  }
}