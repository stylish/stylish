{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
			"semi": [2, "never"],
			"space-infix-ops": 0,
			"max-len": [2, 120, 2]
    }
}
