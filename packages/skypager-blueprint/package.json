  "license": "SEE LICENSE IN License.md",
  "skypager": {
    "main": "skypager.js",
    "plugins":[
      "blueprints"
    ],
    "provides": [
      "product-roadmap"
    ]
  },
  "devDependencies": {
    "skypager-project": "latest",
    "babel-preset-skypager": "latest",
    "mocha": "latest",
    "should": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skypager/skypager"
  },
  "bugs": {
    "url": "https://github.com/skypager/skypager"
  },
  "homepage": "https://skypager.io"
